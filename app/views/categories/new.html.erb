<h2>Add New Category</h2>

<%= form_for @category do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %> <br>
  </div>

  <%= f.submit 'Submit', class: 'btn btn-default' %>
  <%= link_to 'Cancel', categories_path, class: 'btn btn-default' %>

<% end %>